<div>
    <main>
        <div>
            {{#with page_header}}
                <h1 class="text-center">{{title}}</h1>
                <br />
                <p class='text-uppercase text-center'>
                    {{description}}
                </p>
            {{/with}}
        </div>
    <section>
        {{#with section}}
            <section class='card'>
                <div class="card-body">
                {{#with about_us}}
                    <div>
                        {{#with title_list}}
                            <div class='display-6 text-end'>
                                {{main}}
                            </div>
                            <br />
                            <h2 class='text-end'>
                                {{sub}}
                            </h2>
                        {{/with}}
                        <br />
                        <div>
                        {{#with button}}
                            <button
                                class='btn btn-outline-info'
                            >{{text}}</button>
                        {{/with}}
                    </div>
                {{/with}}
                </div>
            </section>
            <br>
            <section class="card">
                <div class="card-body">
                    {{#with advantage}}
                        <div class='display-5'>
                            {{title}}
                        </div>
                        <br>
                        <section class="card">
                            <div class="card-body">
                                {{#with efficiency}}
                                    <div class="display-4">
                                        {{title}}
                                    </div>
                                    <br>
                                    <h6>{{sub_title}}</h6>
                                    <br>
                                    <p class="text-secondary">
                                        {{description}}
                                    </p>
                                    {{> warning button=button}}
                                {{/with}}
                            </div>
                        </section>
                        <br>
                        <section class="card">
                            <div class="card-body">
                                {{#with innovation}}
                                    <div class="display-4 text-center">
                                        {{title}}
                                    </div>
                                    <br>
                                    <h6 class="text-center">
                                        {{sub_title}}
                                    </h6>
                                    <br>
                                    <p class="text-muted text-center">
                                        {{description}}
                                    </p>
                                    {{> warning button=button}}
                                {{/with}}
                            </div>
                        </section>
                        <br>
                        <section class="card">
                            <div class="card-body">
                                {{#with collaboration}}
                                    <div class='display-4 text-end'>
                                        {{title}}
                                    </div>
                                    <br>
                                    <h6 class="text-end">
                                        {{sub_title}}
                                    </h6>
                                    <br>
                                    <p class="text-muted text-end">
                                        {{description}}
                                    </p>
                                    {{> warning button=button}}
                                {{/with}}
                            </div>
                        </section>
                    {{/with}}
                </div>
            </section>
            <br>
            <section class="card">
                <div class="card-body">
                    {{#with agreement}}
                        <div class="display-5">
                            {{title}}
                        </div>
                        <br>
                        <section class="card">
                            <div class="card-body">
                                {{#with info}}
                                    <section class="display-5">
                                        {{sub_title}}
                                    </section>
                                    <br>
                                    <section class="card">
                                        <div class="card-body">
                                            {{#with detail}}
                                                <p class="text-secondary">
                                                    {{text}}
                                                </p>
                                                <button class="btn btn-success">
                                                    {{button}}
                                                </button>
                                            {{/with}}
                                        </div>
                                    </section>
                                {{/with}}
                            </div>
                        </section>
                    {{/with}}
                </div>
            </section>
        {{/with}}
    </section>
  </main>
</div>